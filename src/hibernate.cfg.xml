<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory>
  <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
  <property name="hibernate.connection.password">imangine</property>
  <property name="hibernate.connection.url">
  
  <![CDATA[
	jdbc:mysql://localhost:3306/imangine?useUnicode=true&characterEncoding=utf8
	]]>
  </property>
  <property name="hibernate.connection.username">imangine</property>
  <property name="hibernate.dialect">org.hibernate.dialect.MySQL5InnoDBDialect</property>
  <mapping resource="imangine/AlbumIncluded.hbm.xml"/>
  <mapping resource="imangine/AlbumLiked.hbm.xml"/>
  <mapping resource="imangine/Albums.hbm.xml"/>
  <mapping resource="imangine/Friends.hbm.xml"/>
  <mapping resource="imangine/GroupComment.hbm.xml"/>
  <mapping resource="imangine/GroupMem.hbm.xml"/>
  <mapping resource="imangine/Groups.hbm.xml"/>
  <mapping resource="imangine/PicComment.hbm.xml"/>
  <mapping resource="imangine/PicTag.hbm.xml"/>
  <mapping resource="imangine/PicLiked.hbm.xml"/>
  <mapping resource="imangine/Pictures.hbm.xml"/>
  <mapping resource="imangine/Users.hbm.xml"/>
 </session-factory>
</hibernate-configuration>
